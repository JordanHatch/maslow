<%= f.inputs do %>
  <%= f.input :response_type, as: :radio %>
  <%= f.input :name %>
  <%= f.input :url %>

  <%= f.actions do %>
    <%= f.action :submit, button_html: {
                            class: 'btn btn-primary'
                          } %>
    <%= f.action :cancel, as: :link %>
  <% end %>
<% end %>
